{
  "name": "untamper-sdk",
  "version": "1.0.0",
  "description": "Official Node.js SDK for unTamper - Enterprise audit logging platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:example": "ts-node --project tsconfig.node.json examples/simple-test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "untamper",
    "audit",
    "logging",
    "sdk",
    "node",
    "typescript",
    "enterprise"
  ],
  "author": "unTamper Team <oren@untamper.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oyagev/unTamper-sdk-node.git"
  },
  "bugs": {
    "url": "https://github.com/oyagev/unTamper-sdk-node/issues"
  },
  "homepage": "https://github.com/oyagev/unTamper-sdk-node#readme",
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
